<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />

    <title>tourBD - @ViewBag.Title</title>

    <!-- Font Awesome Icons -->
    <link href="~/admin/plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/admin/css/adminlte.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Fira+Sans:wght@300;400;600&display=swap"
          rel="stylesheet" />
    @RenderSection("styles", false)
    <!-- please remove this to proper place before deploying in production-->
    <div id="fb-root"></div>
    <script async
            defer
            crossorigin="anonymous"
            src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v5.0"></script>
    <meta name="google-signin-client_id"
          content="46218180.1366882461.apps.googleusercontent.com" />
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <!-- /please remove this to proper place before deploying in production-->
</head>
<body class="hold-transition bg-light">
    <!--top nav-->
    <div class="container-fluid border-bottom">
        <div class="navbar navbar-expand">
            <!-- Brand Logo -->
            <a href="https://devskill.com" class="border-light brand-link">
                @*<img src="~/img/tourBD-sm.PNG"
                     alt="tourBD"
                     class="brand-image-xl logo-xs" />*@
                <img src="~/img/tourBD-big.PNG"
                     alt="tourBD"
                     class="brand-image-xs logo-xs" />
            </a>
            <ul class="ml-auto mr-sm-auto navbar-nav">
                <li class="nav-item">
                    <a href="#" class="nav-link text-body text-lg font-weight-bold">HOME</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link text-body text-lg font-weight-bold">FORUM</a>
                </li>
                <li class="nav-item">
                    <a href="#" class="nav-link text-body text-lg font-weight-bold">FAQ</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link text-body text-lg font-weight-bold">CONTACT</a>
                </li>
            </ul>
        </div>
    </div>
    <!--/top nav-->
    @RenderBody()
    <!-- Main Footer -->
    <footer class="bg-light border-top container-fluid p-3" style="height:auto">
        <div class="d-flex flex-row">
            <div>© 2020 <a href="#">tourBD</a></div>
            <div class="text-right col">
                @*<a href="facebook.com/devskillbd">
                    <img width="100"
                         src="/img/FindUs-FB-RGB.svg"
                         alt="Facebook Page" />
                </a>*@
            </div>
        </div>
    </footer>
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/admin/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/admin/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/admin/js/adminlte.min.js"></script>
    @RenderSection("Scripts", false)
</body>
</html>
